<p id="notice"><%= notice %></p>

<h1>Coupons</h1>

<table>
  <thead>
    <tr>
      <th>Coupon Code</th>
      <th>Store</th>
      <th colspan="3"></th>
    </tr>
  </thead>

    <tbody>
    <% @coupons.each do |c| %>
      <tr>
        <%# <td><%= link_to c.name, coupon %>
        <td><%= c.coupon_code %></td>
        <td><%= c.store %></td>
        <% <td><%= link_to 'Show', coupon %>
        <td><%= link_to 'Edit', edit_pet_path(c) %></td>
        <td><%= link_to 'Destroy', coupon, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

